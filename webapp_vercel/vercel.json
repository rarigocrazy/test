{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/**/*.js": {
      "runtime": "@vercel/node",
      "maxDuration": 30
    }
  },
  "env": {
    "CRYPTO_BOT_TOKEN": "@crypto_bot_token",
    "WEBAPP_URL": "@webapp_url"
  }
}